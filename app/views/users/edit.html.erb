
<h2> Edit your Profile</h2>


<%= form_tag "/users", method: :patch do %>


<div class="text-center row">


      <div>
        <%= text_field_tag :first_name, nil, placeholder: "First Name" %>
      </div>

    <div>
      <%= text_field_tag :last_name, nil, placeholder: "Last Name" %>
    </div>

   <div>
      <%= text_field_tag :email, nil, placeholder: "Email" %>
    </div>


    <div>
      <%= text_field_tag :phone, nil, placeholder: "Control" %>
    </div>


     <div>
      <%= text_field_tag :state, nil, placeholder: "State" %>
    </div>


    <div>
      <%= text_field_tag :zip_code, nil, placeholder: "Zip_code" %>
    </div>


     <div>
      <%= text_field_tag :street, nil, placeholder: "Zip_code" %>
    </div>


     <div>
      <%= text_field_tag :password, nil, placeholder: "Password" %>
    </div>



  <%=submit_tag %>
  
<% end %>
  